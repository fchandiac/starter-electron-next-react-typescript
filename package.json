{
  "name": "starter-electron-next-react-typescript",
  "productName": "AppName",
  "version": "0.1.0",
  "private": true,
  "main": "main.ts",
  "scripts": {
    "dev:next": "next dev",
    "dev:electron": "tsc && electron dist/main.js",
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:electron\"",
    "build:electron": "tsc -p tsconfig.json",
    "build": "next build && npm run build:electron",
    "build:mac": "electron-builder --mac --config electron-builder.json",
    "build:win": "electron-builder --win --config electron-builder.json",
    "start": "next start"
  },
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "concurrently": "^9.2.0",
    "electron": "^37.2.6",
    "get-port": "^7.1.0",
    "next": "15.3.3",
    "next-auth": "^4.24.11",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/electron": "^1.4.38",
    "@types/node": "^20.19.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/sqlite3": "^3.1.11",
    "electron-builder": "^26.0.12",
    "tailwindcss": "^4.1.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
